    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app.js"
        },
        {
            "name": "Launch Import Current Season",
            "program": "${workspaceFolder}/importers/import-season.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "Launch Import Playoffs",
            "program": "${workspaceFolder}/importers/import-playoffs.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Spawn Matches Week 15",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/util/spawn-matches.js",
            "args": ["15"]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Generate Stats",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/util/compute-stats.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Summarize Week",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/util/summarize-weeks.js"
        }
    ]
}